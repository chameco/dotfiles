source ~/.mutt/colors

macro index <f2> '<change-folder>imaps://chame.co<enter>'
macro index <f3> '<change-folder>imaps://mail.rpi.edu<enter>'

folder-hook 'chame.co' 'source ~/.mutt/account.personal'
folder-hook 'mail.rpi.edu' 'source ~/.mutt/account.school'
# switch to default account on startup
source ~/.mutt/account.personal
